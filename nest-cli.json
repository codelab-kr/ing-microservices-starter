{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/videos/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/videos/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/videos",
  "videos": {
    "type": "application",
    "root": "apps/videos",
    "entryFile": "main",
    "sourceRoot": "app/videos/src",
    "compilerOptions": {
      "tsConfigPath": "apps/videos/tsconfig.app.json"
    }
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "metadata": {
      "type": "application",
      "root": "apps/metadata",
      "entryFile": "main",
      "sourceRoot": "apps/metadata/src",
      "compilerOptions": {
        "tsConfigPath": "apps/metadata/tsconfig.app.json"
      }
    },
    "history": {
      "type": "application",
      "root": "apps/history",
      "entryFile": "main",
      "sourceRoot": "apps/history/src",
      "compilerOptions": {
        "tsConfigPath": "apps/history/tsconfig.app.json"
      }
    }
  }
}